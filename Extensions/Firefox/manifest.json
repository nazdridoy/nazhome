{
  "manifest_version": 3,
  "name": "nazHome",
  "version": "1.0.0",
  "description": "Set nazHome as your homepage and new tab page",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "nazhome@nazdridoy.github.io",
      "strict_min_version": "109.0"
    }
  },
  "chrome_settings_overrides": {
    "homepage": "https://nazhome.pages.dev"
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'; frame-src https://nazhome.pages.dev"
  }
} 
